#!/usr/bin/env testml-boot

Diff = 1
*input.compile == *output

=== Code objects: string, number, regex, point, list, function
--- input
a = 'string'
b = "string\n"
c = 42
d = -3.14
e = /foo\/.*/
f = *foo
g = ['this', -42, /.*/, *foo, [], [1], [2, 3]]
# h = (a, b)=>

--- output
{ "testml": "0.3.0",
  "code": [
    ["=","a","string"],
    ["=","b","string\n"],
    ["=","c",42],
    ["=","d",-3.14],
    ["=","e",
      ["/","foo\\/.*"]],
    ["=","f",
      ["*","foo"]],
    ["=","g",
      ["[]",
        "this",
        -42,
        ["/",".*"],
        ["*","foo"],
        ["[]"],
        ["[]",1,null],
        ["[]",2,3]]]],
  "data": []}

